@using EPlast.DataAccess.Entities
@model EPlast.ViewModels.RegionsAdmins

<form class="container bg-light m-5 pt-2 pb-5 pr-5 pl-5" style="background-color:white" asp-action="EditConfirmed" enctype="multipart/form-data">
    <div class="row">
        <div class="col-7">
            <div class="form-group">
                @Html.TextBoxFor(x=>x.City,
                new { @list = "CityType-value", @id = "regionsAndAdmins", @class = "form-control", @Value = "" })
                <datalist id="CityType-value">
                    @foreach (City item in Model.Cities)
                    {
                        <option data-value="@item.ID" value="@item.Name"></option>
                    }
                </datalist>
            </div>
        </div>
    </div>
    <div class="row">
        <div id="getAdmins"></div>
    </div>
</form>
